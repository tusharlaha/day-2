//program to add first n natural numbers
let sum=0
let n=prompt("enter your number")
n=Number.parseInt(n)
for(let i=0;i<n;i++){
    sum += (i+1)
}
console.log("sum of first" +n + "natural numbers is " +sum)
//for in loops
let obj={
    tushar:95,
    shubham:22,
    aniket:55,
    shivam:45,
    vishal:67,
}
for(let a in obj){
    console.log(" marks of " +a + " is " +obj[a] )
}
//for of loops
for(let b of "tushar"){
    console.log(b)
}
//while loop
let no =prompt("enter your value")
no=Number.parseInt(no)
i=0
while(i<no){
console.log(i)
i++
}
//do while loop
let nom =prompt("enter your value")
nom=Number.parseInt(nom)
i=0
do{
console.log(i)
i++;
}
while(i<nom)
