//comparison operator
let comp1=8;
let comp2="8";
console.log("comp1==comp2 is",comp1==comp2)
console.log("comp1!=comp2 is",comp1!=comp2)
console.log("comp1===comp2 is",comp1===comp2)
console.log("comp1!==comp2 is",comp1!==comp2)
