// //conditional  operator
let age = prompt("hey what's your age?");

age=Number.parseInt(age)//converting string into number(typecasting)
if(age<0){
    alert("this is a valid age");
}
if(age<9){
    alert("you are a kid and you cannot think of driving in this age");
}
else if(age<18 && age>=9){
    alert("you are a kid and you can think of driving after age 18");
}
else{
    alert("you can now drive above age 18");
}
console.log("you can", (age<18? "not drive" : "drive"))
//switch case 
const expr = 'guava'
switch (expr){
case 'mango':
console.log("mangoes are 50rs/kg")
break;
case'guava':
case 'banana':
console.log("guava and bananas are 180rs/kg");
break;
default:
console.log("there are no more fruits")
}
